<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:isScrollContainer="true"
    android:orientation="vertical"
    android:scrollbarAlwaysDrawHorizontalTrack="true"
    android:scrollbars="horizontal"
    android:name=".VanSelling.VansellingReturnNew">

    <include layout="@layout/maintoolbar" />

    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:isScrollContainer="true"
            android:scrollbarAlwaysDrawVerticalTrack="true"
            android:scrollbars="vertical"
            android:stretchColumns="1">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:padding="20dp"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <!-- Customer Code Section -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="New VanSelling Return"
                        android:textStyle="bold"
                        android:textSize="14sp"
                        tools:ignore="HardcodedText" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"
                        android:layout_marginVertical="5dp" />

                    <!-- Customer Name Section -->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Salesman"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:id="@+id/tvsalesmanvanretnew"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/rounded_textview"
                        android:gravity="center_vertical"
                        android:padding="8dp"
                        android:hint="Salesman Name"
                        android:textSize="14sp"
                        tools:ignore="HardcodedText" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"
                        android:layout_marginVertical="5dp" />

                    <!-- Return Invoice Details Header -->
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:backgroundTint="@color/darker_shade">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="12dp"
                            android:text="Return Details"
                            android:textColor="@color/white"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.cardview.widget.CardView>

                    <!-- select product -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        app:columnCount="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Select Return Type"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select product item -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="1">

                        <Spinner
                            android:id="@+id/sprettypevr"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:entries="@array/returntype"
                            android:background="@drawable/spinner_light" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select outlet, category, delivery date, search item, barcode button card view -->
                    <!-- select outlet -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Select Outlet"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <Spinner
                            android:id="@+id/spoutletnewinvvr"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/spinner_light"
                            android:entries="@array/outlet"
                            app:layout_columnWeight="1" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select category -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Category"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <Spinner
                            android:id="@+id/spcategorynewinvvr"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/spinner_light"
                            android:entries="@array/outlet"
                            app:layout_columnWeight="1" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- search item -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Item Description"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etqueryproductvr"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="5dp"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="Search Item"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="1"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- barcode -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Barcode"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etquerybarcodevr"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="5dp"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="Input Barcode"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="1"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- barcode button section -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:columnCount="2">

                        <Button
                            android:id="@+id/btscanbarcode"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:layout_marginEnd="5dp"
                            android:text="Item B-Code"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText" />

                        <Button
                            android:id="@+id/btscanbarcodecs"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:text="Case B-Code"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select product -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        app:columnCount="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Select Product"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select product item -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="1">

                        <Spinner
                            android:id="@+id/spproductnewvr"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="@drawable/spinner_light" />
                    </androidx.gridlayout.widget.GridLayout>

                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="4">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Case Factor"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/tvcsfactornewinvoicevr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edittext_light"
                            android:hint="0"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText,TextViewEdits,DuplicateSpeakableTextCheck" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- unit of measure and cases -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        app:columnCount="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Unit Of Measure&amp;Cases"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="4">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="UM"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/tvumnewinvoicevr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="0"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="1"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:text="CS-Unit"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/tvcsumnewinvoicevr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="0"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="3"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- unit price and cases -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        app:columnCount="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity&amp;Cases"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="4">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="QTY"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etquantitynewvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="0"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="1"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:text="CS-QTY"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etcsquantitynewvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="0"
                            android:inputType="textMultiLine"
                            android:textSize="14sp"
                            android:padding="8dp"
                            app:layout_column="3"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- select product buttons section -->
                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2"
                        app:rowCount="1">

                        <Button
                            android:id="@+id/btexpirydatenewvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:text="EXPIRATION DATE"
                            android:layout_marginEnd="5dp"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            app:layout_column="0"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/tvexpirydatenewvr"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:background="@drawable/rounded_textview"
                            android:gravity="center_vertical"
                            android:layout_marginStart="5dp"
                            android:padding="8dp"
                            android:hint="M/D/YEAR"
                            android:textSize="14sp"
                            android:textColor="@color/main_color"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            app:layout_column="1"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>


                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:columnCount="2">

                        <Button
                            android:id="@+id/btadddetailsnewinvvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:layout_marginEnd="5dp"
                            android:text="Add to Returns"
                            app:layout_column="0"
                            app:layout_columnWeight="1"
                            app:layout_row="0"
                            tools:ignore="HardcodedText" />

                        <Button
                            android:id="@+id/btcleardetailsvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:layout_marginStart="5dp"
                            android:text="Clear Details"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <androidx.gridlayout.widget.GridLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        app:columnCount="2">

                        <Button
                            android:id="@+id/btadditemvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:text="Add Item"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText" />

                        <Button
                            android:id="@+id/btdeleteitemnewvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/bg_button"
                            android:textColor="@drawable/button_text_selector"
                            android:text="Delete Item"
                            app:layout_columnWeight="1"
                            tools:ignore="HardcodedText" />
                    </androidx.gridlayout.widget.GridLayout>

                    <!-- Invoice Details Header -->
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/darker_shade">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="12dp"
                            android:text="Return Details"
                            android:textColor="@color/white"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </androidx.cardview.widget.CardView>

                    <!-- Table Header Row -->
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/dark_gray">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:text="Qty"
                                android:textColor="@color/white"
                                android:textSize="11sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/textView2"
                                android:layout_width="0dp"
                                android:layout_weight="3"
                                android:layout_height="wrap_content"
                                android:text="Description"
                                android:textColor="@color/white"
                                android:textSize="11sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:text="UM"
                                android:textColor="@color/white"
                                android:textSize="11sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:text="EX-Date"
                                android:textColor="@color/white"
                                android:textSize="11sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                tools:ignore="HardcodedText" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <!-- Table Input Row -->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="2dp">

                        <EditText
                            android:id="@+id/etqtynewvr"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:hint="qty"
                            android:background="@drawable/edittext_light"
                            android:inputType="textMultiLine"
                            android:textSize="11sp"
                            android:gravity="center"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etdecriptionnewvr"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:autofillHints=""
                            android:background="@drawable/edittext_light"
                            android:hint="description"
                            android:inputType="textMultiLine"
                            android:textSize="11sp"
                            android:gravity="center"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etumnewvr"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:hint="um"
                            android:background="@drawable/edittext_light"
                            android:inputType="textMultiLine"
                            android:textSize="11sp"
                            android:gravity="center"
                            tools:ignore="HardcodedText" />

                        <EditText
                            android:id="@+id/etexpirynewvr"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:autofillHints=""
                            android:hint="ex-date"
                            android:background="@drawable/edittext_light"
                            android:inputType="textMultiLine"
                            android:textSize="11sp"
                            android:textColor="@color/main_color"
                            android:gravity="center"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/edittext_light"
                        android:orientation="vertical"
                        android:padding="12dp">

                        <!-- Total Quantity Row -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="center_vertical"
                            tools:ignore="UselessParent">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:text="Total Qty"
                                android:textSize="14sp"
                                android:textColor="@color/black"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvtotalinvoiceqtyvr"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="end"
                                android:text="0"
                                android:textColor="@color/black"
                                android:textSize="14sp"
                                tools:ignore="HardcodedText" />
                        </LinearLayout>
                    </LinearLayout>

                    <Button
                        android:id="@+id/btsendsavevr"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="50dp"
                        android:layout_marginTop="5dp"
